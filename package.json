{
	"name": "vlp",
	"displayName": "Valdese Lakeside Park",
	"version": "3.9",
	"description": "Navigating the new Lakeside Park in Valdese",
	"scripts": {
		"gen-leafletcss": "node bin/genLeafletCSS.js",
		"build": "webpack --mode production",
		"serve": "http-server -p 1336 build -c-1 --gzip",
		"zakklab-build": "webpack --env ZAKKLAB=1 --mode production",
		"zakklab-test": "webpack --env ZAKKLAB=1 --mode development",
		"convert-shapefile": "http-server -p 8080 shapefiles -c-1"
	},
	"author": "Valdese Network",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/valdese-net/vlp.git"
	},
	"dependencies": {
		"@mdi/js": "^7.3.67",
		"dayjs": "^1.11.10",
		"leaflet": "^1.9.4",
		"leaflet-measure": "^3.1.0",
		"leaflet.featuregroup.subgroup": "^1.0.2",
		"leaflet.markercluster": "^1.5.3",
		"navigo": "7.1.2",
		"yaml": "^2.3.4"
	},
	"devDependencies": {
		"compression-webpack-plugin": "^10.0.0",
		"css-loader": "^6.8.1",
		"html-loader": "^4.2.0",
		"html-webpack-plugin": "^5.5.3",
		"http-server": "^14.1.1",
		"marked": "^4.3.0",
		"mini-css-extract-plugin": "^2.7.6",
		"p-iteration": "^1.1.8",
		"resolve-url-loader": "^5.0.0",
		"sass": "^1.69.5",
		"sass-loader": "^13.3.2",
		"twing": "^5.1.4",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.1.4",
		"workbox-webpack-plugin": "^7.0.0"
	}
}
