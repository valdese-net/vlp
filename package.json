{
  "name": "vlp",
  "displayName": "Valdese Lakeside Park",
  "version": "0.9.8",
  "description": "Navigating the new park and showing park plan overlays",
  "scripts": {
    "build": "webpack --mode production",
    "test": "webpack --mode development",
    "serve": "http-server -p 1336 build -c-1",
