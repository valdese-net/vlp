		"@turf/turf": "^6.5.0",
		"dayjs": "^1.11.18",
		"leaflet": "^1.9.4",
		"leaflet-ant-path": "^1.3.0",
		"leaflet.featuregroup.subgroup": "^1.0.2",
		"leaflet.markercluster": "^1.5.3",
		"leaflet.motion": "^0.3.2",
		"maplibre-gl": "^4.7.1",
		"navigo": "7.1.2",
		"pmtiles": "^3.2.1",
		"protomaps-leaflet": "^3.1.2",
		"yaml": "^2.8.1"
