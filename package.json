		"p-iteration": "^1.1.8",
		"resolve-url-loader": "^5.0.0",
		"sass": "^1.93.2",
		"sass-loader": "^13.3.3",
		"shpjs": "^5.0.2",
		"twing": "^5.2.2",
		"webpack": "^5.101.3",
