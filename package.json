  },
  "dependencies": {
    "@mdi/js": "^5.3.45",
    "date-fns": "^2.13.0",
    "leaflet": "^1.5.1",
    "leaflet.featuregroup.subgroup": "^1.0.2",
    "leaflet.markercluster": "^1.4.1"
  },
  "devDependencies": {
    "markdown-loader": "^5.1.0",
    "raw-loader": "^1.0.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "workbox-webpack-plugin": "^5.1.3"
